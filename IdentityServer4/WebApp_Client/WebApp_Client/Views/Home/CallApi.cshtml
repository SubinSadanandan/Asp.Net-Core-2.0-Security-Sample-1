

<div>
    Logged in User: <strong>@User.Identity.Name</strong>
</div>
<br/>

<div>
    @if (ViewBag.Error != null)
    {
        <text>@ViewBag.Error</text>
    }
    else
    {
        <text>Response from API: @ViewBag.ApiResponse</text>
    }
</div>
<br />

<div>
    AccessToken: @ViewBag.AccessToken
</div>
<br/>

<div>
    RefreshToken: @ViewBag.RefreshToken
</div>